apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: ${ca_cert}
    server: "${url}"
  name: "${prefix}"
contexts:
- context:
    cluster: "${prefix}"
    user: admin
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: system:node:${name}
  user:
    client-certificate-data: ${cert}
    client-key-data: ${key}
